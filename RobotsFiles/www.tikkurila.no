}

function closePrivacypolicyDetails() {
$('.privacypolicy-details').removeClass('visible');
}

function showPrivacypolicyDetails() {
$('.privacypolicy-details').addClass('visible');
}

$('a.show-privacypolicy-details').on('click', function(event){
event.preventDefault();
showPrivacypolicyDetails();
return false;
});

function checkCookieDisclaimerCookie(){
if (document.cookie.indexOf('TIKKURILA_COMDISCLAIMER') == -1) {
$('.cookiedisclaimer').show();
} else {
$('.cookiedisclaimer').remove();
}
}
checkCookieDisclaimerCookie();
