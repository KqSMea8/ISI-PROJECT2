$i('#plflag').hide();


}
});


$i(".dropdown img.flag").addClass("flagvisibility");
$i(".dropdown dt a").click(function () {
$i(".dropdown dd ul").toggle();
});

$i(".dropdown dd ul li a").click(function () {

var text = $i(this).html();
$i(".dropdown dt a span").html(text);
$i(".dropdown dd ul").hide();
var itemId = $i("#locale").find("dt a span.value").html();
$i("#culture").val(itemId);
$i("#culture").parents("form").submit();

});

$i(document).bind('click', function (e) {
var $iclicked = $i(e.target);
if (!$iclicked.parents().hasClass("dropdown"))
$i(".dropdown dd ul").hide();
});

