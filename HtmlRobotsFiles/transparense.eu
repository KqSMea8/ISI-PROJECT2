<script type="text/javascript">

function getCountryTitle(country) {
countries = {
"eu": ["&nbsp;"],
"pt": ["Portugal"],
"es": ["Spain"],
"fr": ["France"],
"nl": ["Netherlands"],
"de": ["Germany"],
"cz": ["Czech Republic"],
"at": ["Austria"],
"si": ["Slovenia"],
"hr": ["Croatia"],
"ro": ["Romania"],
"mt": ["Malta"]
}

return countries[country];
}

function highlight(country) {

// hiding others
$('.glass').hide();

// showing one
var id = '#glass-' + country
$(id).show();

// disabling all
$("#maps .countries").css('display', 'none');

// enabling one
var element = '#' + country
$(element).css('display', 'block');

// sessing glass imagemap href
var href = '/' + country  +'/'
$('#glassHref').attr('href', href);
}

function turnoff() {
// hiding all
$('.glass').hide();

// disabling all
$("#maps .countries").css('display', 'none');
}

$(document).ready(function () {
$("#maps .countries").css('display', 'none');
});

</script>


