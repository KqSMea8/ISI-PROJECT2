$(function(){
//Farbe beim MouseOver toggeln
$(".back-to-top").mouseenter(function(){
$(this).animate({
color: "#fff",
opacity: 1
}, 200);
}).mouseleave(function(){
$(this).animate({
color: "#282828",
opacity: 0.75
}, 200);
});

//Scrollverhalten
$(".back-to-top").click(function(){
$("html, body").animate({ scrollTop: 0}, 'slow');
$(this).mouseleave();
});

//Element erst ab Scroll-Offset einblenden
$(document).scroll(function(){
var y = $(this).scrollTop();
if(y > 100){
$(".back-to-top").fadeIn();
}
else{
$(".back-to-top").fadeOut();
}
});
});
//
</script>




