<script type="text/javascript">
//
$(function(){
//Positionierung des Skyscrapers
$(window).resize(alignSkyscraper);
alignSkyscraper()
});

function alignSkyscraper(){
var winWidth = $(window).width();
var pageObj=$("main.page");
var adzone=$(".element_adzone");
var posPage = pageObj.offset();
var width = parseInt(pageObj.css("width"), 10);
var offset = 9;
var top = posPage.top;
var left = posPage.left + width + offset;

//bei Desktopansicht (1024px + 2 * 120px Skyscraper + 2* offset) neben dem Content anzeigen, sonst darunter
if(winWidth >= (1024 + 2 * 120 + 2 * offset)){
adzone.css("position", "absolute");
adzone.offset({top: top, left: left});
adzone.removeClass("mobile");
}
else{
adzone.css("position", "relative");
adzone.css("top", offset);
adzone.css("left", "0");
adzone.css("margin", "0 auto");
adzone.addClass("mobile");
}
}
//
</script>







<script type="text/javascript">
//
$(function(){
//Positionierung des Blocks
$(window).resize(alignPromotions);
alignPromotions()
});

function alignPromotions(){
var winWidth = $(window).width();
var pageObj=$("main.page");
var promotions=$(".element_promotions");
var posPage = pageObj.offset();
var width = parseInt(pageObj.css("width"), 10);
var offset = 9;
var top = posPage.top;
var adzone = $(".element_adzone");
if(adzone.length > 0){
top = adzone.position().top + adzone.height() + offset;
}
var left = posPage.left + width + offset;

//bei Desktopansicht (1024px + 2 * 200px Skyscraper + 2* offset) neben dem Content anzeigen, sonst darunter
if(winWidth >= (1024 + 2 * 200 + 2 * offset)){
promotions.css("position", "absolute");
promotions.offset({top: top, left: left});
promotions.removeClass("mobile");
}
else{
promotions.css("position", "relative");
promotions.css("top", offset);
promotions.css("left", "0");
promotions.css("margin", "0 auto");
promotions.addClass("mobile");
}
}
//
</script>






