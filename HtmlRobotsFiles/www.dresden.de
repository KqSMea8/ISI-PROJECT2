$(function(){
$(window).resize(resize2Column);
resize2Column();
});
function resize2Column()
{
//alle direkten Teaserblocke suchen
$(".element_navigation_inline .teaser_blocks").each(function () {
$(this).children(".teaser_row").each(function () {
$(this).children(".teaser_resize_element").each(function (index) {
//Nachbar im 2-spaltigen Layout ermitteln
if (index % 2 === 0)
{
var neighbor = $(this).next(".teaser_resize_element");
if (neighbor !== null)
{
//nur verarbeiten, wenn es sich nicht um colgroups oder Bilder handelt
if($(this).attr("class") !== undefined && neighbor.attr("class") !== undefined)
{
if (($(this).attr("class").indexOf("teaser_image") === -1) && (neighbor.attr("class").indexOf("teaser_image") === -1))
{
//Hohen-Attribute auf 0 setzen, um Neurendern anzustossen und anschliessend entfernen
$(this).height("auto");
neighbor.height("auto");
$(this).removeAttr("height");
neighbor.removeAttr("height");

if ($(document).width() >= 520 && $(document).width() < 900)
{
//Hohe der benachbarten Elemente gleich setzen
if ($(this).height() > neighbor.height())
neighbor.height($(this).height());
else if ($(this).height() < neighbor.height())
$(this).height(neighbor.height());
}
}
}
}
}
});
});
});
}
//
</script>

