$(this).children(".teaser_row").each(function(){
$(this).children(".teaser_mobile_hide").each(function () {
teaserElements.push($("<div class=\"teaser empty\"/>"));
});
});
//alte Zeilen entfernen
$(this).children(".teaser_row").remove();
//Elemente auf Zeilen aufteilen und ausgeben
