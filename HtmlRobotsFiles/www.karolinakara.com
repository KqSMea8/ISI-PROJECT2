<script type="text/javascript">
count = $(".slide").length;
p = count - 1;
n = count - 2;
var curr = 0;
var i = 0;
while (i < count) {
$('.slider-buttons').append("<a href='' class='slide-btn color-btn slide" + i + "' data-curr=" + (i - 1) + "><div class='long-btn'></div></a>");
i++;
}
$('.color-btn').click(function (e) {
e.preventDefault();
$(this).css('background-color', '#78c4ab').siblings().css('background-color', 'white');
curr = $(this).data('curr');
if (curr > n) {
curr = 0;
} else {
curr++;
}
$(".slide-container").css({
marginLeft: curr * -100 + "%"
});
});

// Sets the slide-container and individual slide size
$(".slide-container").css("width", 100 * count + "%");
$(".slide").css("width", 100 / count + "%");

// Conditional statement for clicking next
$(".next").click(function (e) {
e.preventDefault();
if (curr > n) {
curr = 0;
} else {
curr++;
}
$(".slide-container").css({
marginLeft: curr * -100 + "%"
});
var currentslide = "slide" + curr;
$("." + currentslide).css('color', 'red').siblings().css('color', 'black');

});

// Conditional statement for clicking prev
$(".prev").click(function (e) {
e.preventDefault();
if (curr < 1) {
curr = p;
} else {
curr--;
}
$(".slide-container").css({
marginLeft: curr * -100 + "%"
});
var currentslide = "slide" + curr;
$("." + currentslide).css('color', 'red').siblings().css('color', 'black');
});


// swipe

$(".carousel").swipe({

swipe: function (event, direction, distance, duration, fingerCount, fingerData) {

if (direction == 'left') $(this).carousel('next');
if (direction == 'right') $(this).carousel('prev');

},
allowPageScroll: "vertical"

});

</script>       <!-- Footer -->
