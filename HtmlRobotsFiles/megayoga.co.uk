<script type="text/javascript">
$(function() {
/* GALLERIES */
$('#side_galleries li').tooltip({
delay: 500,
showURL: false,
bodyHandler: function() {
var id = $(this).attr('image_id');
return '<img src="images/preview/'+id+'.jpg" />';
}});

/* SUBSCRIBE */
$("#button_subscribe").click(function() {
var email = $('#email').val();
$('#subscribe input').attr('disabled','disabled');
$('#subscribe .email_error').hide();
$('#subscribe #thankyou').hide();
$.post('http://megayoga.co.uk/pl/cmsajax/add_user',{email:email},function(result) {
$('#subscribe input').attr('disabled','');
if(result.code==1) {
$('#subscribe .email_error').show();
} else {
$('#subscribe #thankyou').show();
}
},'json');
return false;
})
});
</script>
