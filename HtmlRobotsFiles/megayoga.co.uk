<script type="text/javascript">
function show_player() {
$('#player_wrapper').html('<div id="player_main" style="height:200px;"></div>');
var flashvars = {};
flashvars.file = 'http://megayoga.co.uk/pl/player/playlist';
flashvars.playlist = "bottom";
flashvars.showdigits = false;
flashvars.playlistsize = 180;
flashvars.backcolor = "000000";
flashvars.frontcolor = "909090";
flashvars.resizible = false;
//flashvars.skin = "media/mediaplayer/stylish.swf";
var params = {};
params.wmode = "transparent";
params.allowScriptAccess = "always";
var attributes = {};
attributes.id = "player_main";
attributes.name = "player_main";
swfobject.embedSWF("media/mediaplayer/player3.swf", "player_main", "220", "200", "9.0.0","expressInstall.swf", flashvars, params, attributes);
$('#player_detach').show();
}

$(function() {
//window.console.log('player opened:' + $.cookies.get('player_opened'));
if($.cookies.get('player_opened')=='false' || $.cookies.get('player_opened')==null) {
show_player();
}
