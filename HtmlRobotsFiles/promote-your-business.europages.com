$footer_dropdown_menu = $footer_dropdown.children('.footer_dropdown_menu'),
$footer_dropdown_btn = $footer_dropdown.children('.footer_dropdown_toggle');

$footer_dropdown_btn.click(function (e) {
e.preventDefault();
$footer_dropdown.toggleClass('dropdown_open');
});

setTimeout(function() {
$footer_dropdown_menu.css({
'min-width': $footer_dropdown_btn.innerWidth()
});
},200);

$(document).click(function(event) {
if (!$(event.target).closest('.footer_dropdown').length) {
if ($footer_dropdown_menu.is(":visible")) {
$footer_dropdown.removeClass('dropdown_open');
}
}
});
setTimeout(function() {
$('.matchH').setAllToMaxHeight();
},500);
});
